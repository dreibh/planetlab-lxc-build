[base]
name=Scientific Linux 6 - $basearch
baseurl=@MIRRORURL@/linux/scientific/6/$basearch/os/
enabled=1
gpgcheck=1
gpgkey=http://ftp.scientificlinux.org/linux/scientific/6.0/x86_64/os/RPM-GPG-KEY-sl6 http://www.scientificlinux.org/documentation/gpg/RPM-GPG-KEY-dawson

[updates]
name=Scientific Linux 6 - $basearch - security updates
baseurl=@MIRRORURL@/linux/scientific/6/$basearch/updates/security/
enabled=1
gpgcheck=1
gpgkey=http://www.scientificlinux.org/documentation/gpg/RPM-GPG-KEY-sl http://www.scientificlinux.org/documentation/gpg/RPM-GPG-KEY-dawson

[epel]
name=Extra Packages for Enterprise Linux 6 - $basearch
baseurl=@MIRRORURL@/epel/6/$basearch/
enabled=1
gpgcheck=1
gpgkey=http://download.fedora.redhat.com/pub/epel/RPM-GPG-KEY-EPEL-6
